{
    "name": "test1",
    "desc": "Test 1 - Standard Config",
    "result_dir": "./results/",
    "device": "cuda",
    "device_ids": [0],
    "epochs": 1000,
    "resume": false,
    "save_freq": 1,
    "verbosity": 0,
    "seed": 999,
    "trainer": "DefaultTrainer",
    "logger": "VisdomLogger",
    "loss": "binary_crossentropy",
    "metrics": [{
        "acc": "binary_accuracy",
        "precision": "precision",
        "loss": "binary_crossentropy"
        }],
    "datasets": {
        "train": {
          "type": "SiameseDataset",
          "data_path": "datasets/train",
          "batch_size": 32,
          "shuffle": true,
          "augment": false,
          "workers": 4
        },
        "validation": {
          "type": "SiameseDataset",
          "data_path": "datasets/val",
          "batch_size": 32,
          "shuffle": false,
          "augment": false,
          "workers": 1
        }
    },
    "optimizer": {
        "type": "Adam",
        "lr": 0.001,
        "betas": [0.9, 0.999]
    },
    "scheduler": {
        "type": "ReduceLROnPlateau",
        "factor": 0.1,
        "patience": 10,
        "min_lr": 1e-8
    },
    "monitor": {
        "min": "val_loss",
        "early_stop": true
    },
    "model": {
        "type": "PseudoSiamese",
        "ensamble": "SWA"
    }
}
